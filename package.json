    "howler": "^2.1.2",
    "jquery": "^3.4.1",
    "laravel-mix": "^5.0.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "sweetalert": "^2.1.2",
    "vue-resource": "^1.5.1",
    "vuelidate": "^0.7.4",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
